<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/buttonFrag1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="41dp"
        android:layout_marginTop="41dp"
        android:text="Frag1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonFrag2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="89dp"
        android:layout_marginTop="41dp"
        android:text="Frag2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <fragment
        android:id="@+id/fragmentD"
        android:name="com.ansari.b18fragments.DFragment"
        android:layout_width="match_parent"
        android:layout_height="350dp"
        android:layout_marginEnd="6dp"
        android:layout_marginTop="33dp"
        app:layout_constraintEnd_toEndOf="@+id/buttonFrag1"
        app:layout_constraintTop_toBottomOf="@+id/buttonFrag1" />


</android.support.constraint.ConstraintLayout>